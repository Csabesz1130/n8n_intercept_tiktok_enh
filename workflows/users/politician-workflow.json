        "jsCode": "const body = $json.body || $json;\nreturn [{json: {user_id: body.user_id || 'politician-001', keyword: body.keyword || '', topics: body.topics || [], organization_type: 'party', region: body.region || 'HU', prompt: body.prompt || '', sentiment_check: true, schedule_time: body.schedule_time || null, channels: body.channels || []}}];"
      },
      "id": "process-request",
      "name": "Process Request",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 300]
    },
