    {
      "parameters": {
        "jsCode": "// Process journalist request\nconst body = $json.body || $json;\n\nreturn [{\n  json: {\n    user_id: body.user_id || 'journalist-001',\n    keyword: body.keyword || body.topic || '',\n    topics: body.topics || [],\n    organization_type: 'journalist',\n    region: body.region || 'HU',\n    prompt: body.prompt || '',\n    fact_check_required: true,\n    publish_channels: body.channels || []\n  }\n}];"
      },
      "id": "process-request",
      "name": "Process Request",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 300]
    },
