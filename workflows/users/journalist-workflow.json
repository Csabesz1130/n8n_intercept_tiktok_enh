      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1.1,
      "position": [1250, 200]
    },
    {
      "parameters": {
        "jsCode": "// Filter ideas that pass fact-check\nconst ideas = $json.ideas || [];\n\nconst factCheckedIdeas = ideas.filter(idea => {\n  // Journalist workflow: prioritize fact-checked content\n  return idea.factCheckPrompts && idea.factCheckPrompts.length > 0;\n});\n\nreturn [{\n  json: {\n    ideas: factCheckedIdeas.length > 0 ? factCheckedIdeas : ideas,\n    fact_check_applied: true,\n    original_count: ideas.length,\n    filtered_count: factCheckedIdeas.length\n  }\n}];"
      },
      "id": "filter-factcheck",
      "name": "Filter by Fact-Check",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1450, 200]
    },
    {
      "parameters": {
        "jsCode": "// Publish to selected channels\nconst ideas = $json.ideas || [];\nconst channels = $items('Process Request')[0]?.json?.publish_channels || [];\n\nif (channels.length === 0) {\n  return [{\n    json: {\n      ideas,\n      published: false,\n      message: 'No channels selected'\n    }\n  }];\n}\n\n// For each idea, prepare for publishing\nconst publishTasks = [];\nfor (const idea of ideas.slice(0, 3)) { // Limit to 3 ideas\n  for (const channel of channels) {\n    publishTasks.push({\n      json: {\n        content: idea.hook || idea.concept || '',\n        channel,\n        idea_id: idea.id\n      }\n    });\n  }\n}\n\nreturn publishTasks;"
      },
      "id": "prepare-publish",
