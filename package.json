{
  "name": "viral-n8n-platform",
  "version": "1.0.0",
  "description": "Viral n8n Content Generation Platform - Full Stack",
  "scripts": {
    "install:all": "npm run install:dashboard && npm run install:scheduler && npm run install:scraper && npm run install:nodes",
    "install:dashboard": "cd dashboard && npm install",
    "install:scheduler": "cd scheduler && npm install",
    "install:scraper": "cd trending-scraper && npm install",
    "install:nodes": "cd nodes && npm install",
    "dev": "npm-run-all --parallel dev:dashboard dev:scheduler dev:scraper",
    "dev:dashboard": "cd dashboard && npm run dev",
    "dev:scheduler": "cd scheduler && npm run dev",
    "dev:scraper": "cd trending-scraper && npm run dev",
    "start": "npm-run-all --parallel start:dashboard start:scheduler start:scraper",
    "start:dashboard": "cd dashboard && npm run dev",
    "start:scheduler": "cd scheduler && npm start",
    "start:scraper": "cd trending-scraper && npm start",
    "build": "cd dashboard && npm run build",
    "setup": "node setup.js",
    "test": "node test-setup.js"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "n8n",
    "content-generation",
    "social-media",
    "automation"
  ],
  "author": "",
  "license": "MIT"
}

